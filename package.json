{
  "name": "ipx",
  "version": "0.9.4",
  "repository": "unjs/ipx",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./bin/ipx.mjs",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
