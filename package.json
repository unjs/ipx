    "sharp": "^0.29.0",
    "ufo": "^0.7.9",
    "xss": "^1.0.10"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/etag": "latest",
    "@types/is-valid-path": "latest",
    "@types/jest": "latest",
    "@types/node-fetch": "latest",
    "@types/sharp": "latest",
    "eslint": "latest",
    "jest": "latest",
    "jiti": "latest",
    "nodemon": "latest",
    "siroc": "latest",
    "standard-version": "latest",
    "ts-jest": "latest",
    "typescript": "latest"
  }
