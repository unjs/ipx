    "sharp": "^0.30.3",
    "ufo": "^0.8.3",
    "xss": "^1.0.11"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/etag": "latest",
    "@types/is-valid-path": "latest",
    "@types/jest": "latest",
    "@types/node-fetch": "latest",
    "@types/sharp": "latest",
    "eslint": "latest",
    "jest": "latest",
    "jiti": "latest",
    "nodemon": "latest",
    "standard-version": "latest",
    "ts-jest": "latest",
    "typescript": "latest",
    "unbuild": "latest"
  }
