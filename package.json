{
  "name": "ipx",
  "version": "1.0.0-2",
  "repository": "unjs/ipx",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": "./bin/ipx.mjs",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nodemon",
    "lint": "eslint --ext .ts . && prettier -c src test",
    "lint:fix": "eslint --ext .ts . --fix && prettier -w src test",
    "prepack": "pnpm build",
    "release": "pnpm test && standard-version && git push --follow-tags && pnpm publish",
    "start": "node bin/ipx.js",
    "test": "pnpm lint && vitest run --coverage"
  },
  "dependencies": {
    "consola": "^2.15.3",
    "defu": "^6.1.2",
    "destr": "^1.2.2",
    "etag": "^1.8.1",
    "image-meta": "^0.1.1",
    "listhen": "^1.0.4",
    "node-fetch-native": "^1.0.2",
    "pathe": "^1.1.0",
    "sharp": "^0.32.0",
    "ufo": "^1.1.1",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/etag": "^1.8.1",
    "@types/is-valid-path": "^0.1.0",
    "@types/sharp": "^0.31.1",
    "@vitest/coverage-c8": "^0.29.8",
    "changelogen": "^0.5.2",
    "eslint": "^8.37.0",
    "eslint-config-unjs": "^0.1.0",
    "jiti": "^1.18.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "serve-handler": "^6.1.5",
    "typescript": "^5.0.3",
    "unbuild": "^1.1.2",
    "vitest": "^0.29.8"
  },
  "packageManager": "pnpm@8.0.0"
}
